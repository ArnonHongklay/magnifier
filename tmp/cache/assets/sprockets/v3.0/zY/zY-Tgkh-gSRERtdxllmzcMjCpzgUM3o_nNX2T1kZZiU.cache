I"ù(function() {
  ohmpieng.directive('diskmonitor', function() {
    return {
      template: '<div></div>',
      scope: {
        chart: '='
      },
      restrict: 'E',
      replace: true,
      link: function(scope, element) {
        var chart, draw;
        chart = new google.visualization.PieChart(element[0]);
        draw = function(datas) {
          var data, options;
          data = google.visualization.arrayToDataTable(datas.data);
          options = {
            title: datas.chart.title
          };
          return chart.draw(data, options);
        };
        return scope.$watch('chart', function(datas) {
          if (datas && datas.data) {
            return draw(datas);
          }
        });
      }
    };
  });

}).call(this);
