<div class="panel panel-default" id="page">
  <div class="row">
    <div class="col-md-4">
      <div class="panel panel-default">
        <div class="panel-heading">
          <i class="icon-beer icon-large"></i>
          General Information
          <div class="panel-tools">
            <div class="btn-group">
              <a class="btn" href="#">
                <i class="fa fa-refresh"></i>
              </a>
            </div>
          </div>
        </div>
        <div class="panel-body">
          <p>OS: Ubuntu 14.04.2 LTS\n3.13.0-43-generic</p>
          <p>Uptime: 46 days 12 hours 43 minutes</p>
          <p>Server Time: Fri May 22 23:37:34 EDT 2015</p>
          <p>Hostname: Madden</p>
        </div>
      </div>
    </div>
    <div class="col-md-4">
      <div class="panel panel-default">
        <div class="panel-heading">
          <i class="icon-beer icon-large"></i>
          IP Address
          <div class="panel-tools">
            <div class="btn-group">
              <a class="btn" href="#">
                <i class="fa fa-refresh"></i>
              </a>
            </div>
          </div>
        </div>
        <div class="panel-body">
          <p>eth0: 128.199.222.111</p>
          <p>eth1: 10.130.247.189</p>
          <p>lo: 127.0.0.1</p>
          <p>external: 128.199.222.111</p>
        </div>
      </div>
    </div>
    <div class="col-md-4">
      <div class="panel panel-default">
        <div class="panel-heading">
          <i class="icon-beer icon-large"></i>
          Disk
          <div class="panel-tools">
            <div class="btn-group">
              <a class="btn" href="#">
                <i class="fa fa-refresh"></i>
              </a>
            </div>
          </div>
        </div>
        <div class="panel-body">
          <meter chart="disk" class="gauge"></meter>
          <div>
            Total: 500 GB
            Used: 250 GB
            Free: 250 GB
          </div>
        </div>
      </div>
    </div>
  </div>
  <!--แถวแรก -->
  <div class="row">

