  <!-- แถวสอง -->
  <div class="row">
    <div class="col-md-6">
      <div class="panel panel-default">
        <div class="panel-heading">
          <i class="icon-beer icon-large"></i>
          ARP Cache
          <div class="panel-tools">
            <div class="btn-group">
              <a class="btn" href="#">
                <i class="fa fa-refresh"></i>
              </a>
            </div>
          </div>
        </div>
        <div class="panel-body">
          <nvd3-line-chart
                data="exampleData"
                id="x1"
                height="300"
                showXAxis="true"
                showYAxis="true"
                tooltips="true"
                interactive="true">
              <svg></svg>
            </nvd3-line-chart>
        </div>
      </div>
    </div>

    <div class="col-md-6">
      <div class="panel panel-default">
        <div class="panel-heading">
          <i class="icon-beer icon-large"></i>
          CPU Information
          <div class="panel-tools">
            <div class="btn-group">
              <a class="btn" href="#">
                <i class="fa fa-refresh"></i>
              </a>
            </div>
          </div>
        </div>
        <div class="panel-body">
          <nvd3-line-chart
              data="exampleData"
              id="x2"
              height="300"
              showXAxis="true"
              showYAxis="true"
              isArea="true"
              tooltips="true"
              interactive="true">
            <svg></svg>
          </nvd3-line-chart>
        </div>
      </div>
    </div>

    <div class="col-md-6">
      <div class="panel panel-default">
        <div class="panel-heading">
          <i class="icon-beer icon-large"></i>
          Memory Information
          <div class="panel-tools">
            <div class="btn-group">
              <a class="btn" href="#">
                <i class="fa fa-refresh"></i>
              </a>
            </div>
          </div>
        </div>
        <div class="panel-body">
          <nvd3-line-chart
              data="isAreaExampleData"
              id="isAreaDataExample"
              height="300"
              showXAxis="true"
              showYAxis="true"
            <svg></svg>
          </nvd3-line-chart>
        </div>
      </div>
    </div>

    <div class="col-md-6">
      <div class="panel panel-default">
        <div class="panel-heading">
          <i class="icon-beer icon-large"></i>
          Swap
